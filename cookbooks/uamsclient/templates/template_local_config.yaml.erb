# Example configuration file used with managed locally Agent
integrations:
  mysql:
    - name: off-host-mysql
      host: <%= @mysql_user %>
      port: 3306
      user: <%= @user %>
      password: "{{.creds.env.MYSQL_DB_PASWORD}}"
      packet-capture-enabled: false
  otel/hostmetrics:
    - name: host-monitoring
