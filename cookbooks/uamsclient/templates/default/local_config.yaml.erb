# Example configuration file used with managed locally Agent
integrations:
  dbo/mysql:
    - name: off-host-mysql
      host: <%= @local_config['mysql_host'] %>
      port: 3306
      user: <%= @local_config['user'] %>
      password:
        value-from:
        provider: aws-secrets-manager
        secret-name: <%= @local_config['secret_name'] %>
      packet-capture-enabled: false
  otel/hostmetrics:
    - name: host-monitoring
